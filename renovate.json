{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "group:allNonMajor",
    ":prHourlyLimitNone"
  ],
  "ignorePresets": [":preserveSemverRanges"],
  "major": {
    "enabled": false
  },
  "pip_requirements": {
    "fileMatch": ["(^|/)REQUIREMENTS$"]
  },
  "rangeStrategy": "bump"
}
